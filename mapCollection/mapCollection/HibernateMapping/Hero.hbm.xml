<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
                  xmlns="urn:nhibernate-mapping-2.2"
                  assembly="mapCollection"
                  namespace="mapCollection">
  <class name="Hero">
    <id name="Id">
      <generator class="guid"/>
    </id>
    <property name="Name"/>
    <property name="MP"/>
    <property name="HP"/>
    <property name="Profession"/>
  </class>
</hibernate-mapping>